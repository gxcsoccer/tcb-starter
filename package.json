{
  "name": "tcb-starter",
  "version": "1.0.0",
  "description": "Tencent CloudBase Starter",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.30.0",
    "egg-scripts": "^2.15.1"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.16.4",
    "egg-ci": "^1.19.0",
    "egg-mock": "^3.26.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^9.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-tcb-starter",
